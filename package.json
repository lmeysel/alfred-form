{
  "name": "alfred-form",
  "version": "0.0.2",
  "description": "",
  "keywords": [],
  "type": "module",
  "main": "dist/alfred-form.umd.cjs",
  "module": "dist/alfred-form.js",
  "exports": {
    ".": {
      "import": "./dist/alfred-form.js",
      "require": "./dist/alfred-form.umd.cjs"
    }
  },
  "typings": "dist/types/alfred-form.d.ts",
  "files": [
    "dist"
  ],
  "author": "lmeysel <lmeysel@noreply.github.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist docs",
    "build": "pnpm run build:compile && pnpm run build:docs",
    "build:compile": "vite build",
    "build:docs": "typedoc --tsconfig tsconfig.lib.json --out docs src/alfred-form.ts",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "test:prod": "pnpm lint && pnpm test",
    "commit": "cz",
    "dev": "vite",
    "precommit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@inertiajs/vue3": "^1.0.7",
    "@types/node": "^18.16.14",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.31.1",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.0",
    "happy-dom": "^9.19.2",
    "kleur": "^4.1.5",
    "lefthook": "^1.4.1",
    "lint-staged": "^13.2.2",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.8.8",
    "replace-in-file": "^6.3.5",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.2",
    "shelljs": "^0.8.5",
    "tslib": "^2.5.2",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "3.x"
  }
}
